  &gt;
    &lt;span
    class=&quot;gatsby-resp-image-background-image&quot;
    style=&quot;padding-bottom: 108.86075949367088%; position: relative; bottom: 0; left: 0; background-image: url(&apos;data:image/png;base64,&apos;); background-size: cover; display: block;&quot;
  &gt;&lt;/span&gt;
  &lt;img
        class=&quot;gatsby-resp-image-image&quot;
        alt=&quot;error&quot;
        title=&quot;error&quot;
        src=&quot;/static/7b63ae617fc03403f8c08481c97a65b6/f058b/error_01.png&quot;
        srcset=&quot;/static/7b63ae617fc03403f8c08481c97a65b6/c26ae/error_01.png 158w,
/static/7b63ae617fc03403f8c08481c97a65b6/6bdcf/error_01.png 315w,
/static/7b63ae617fc03403f8c08481c97a65b6/f058b/error_01.png 630w,
/static/7b63ae617fc03403f8c08481c97a65b6/31aff/error_01.png 823w&quot;
        sizes=&quot;(max-width: 630px) 100vw, 630px&quot;
